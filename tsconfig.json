{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": "./src/",
    "paths": {
      "@/*": ["./*"],
      "@styles/": ["./styles/*"],
      "@components/": ["./components/*"],
      "@layouts/": ["./layouts/*"]
    }
  }
}
